<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_margin="10dp">

    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/fashion_containerclick_recycler_item_card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="8dp"
        app:cardPreventCornerOverlap="true"
        android:background="@drawable/fashion_new_round_border">

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:background="@drawable/rounded_image"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/fashion_containerclick_recycler_item_image"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitXY" />

            <!--<View
                android:id="@+id/blank_view"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="50dp"></View>-->

            <ImageButton
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:background="@color/transparent"
                android:scaleType="fitXY"
                android:src="@drawable/wishlist_semi_trans_back" />

            <ImageButton
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="@color/transparent"
                android:scaleType="fitXY"
                android:src="@drawable/share_semi_trans_back" />
        </RelativeLayout>
    </android.support.v7.widget.CardView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <TextView
        android:id="@+id/fashion_containerclick_recycler_item_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/fashion_containerclick_recycler_item_image"
        android:gravity="center"
        android:lines="1"
        android:fontFamily="@font/alt_regular_webfont"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/fashion_containerclick_recycler_item_price"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/fashion_containerclick_recycler_item_title"
        android:gravity="center"
        android:fontFamily="@font/alt_regular_webfont"
        android:lines="1"
        android:textColor="@color/black" />
    </LinearLayout>
    <!--
</RelativeLayout>-->
</LinearLayout>